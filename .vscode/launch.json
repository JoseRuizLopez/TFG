{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Resumen",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_resumen.py",
      "console": "integratedTerminal",
      "args": []
    },
    {
      "name": "Run Generador",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generator.py",
      "console": "integratedTerminal",
      "args": [
        "--task_id",
        "-1",
        "--MODELO",
        "mobilenet"
      ]
    },
    {
      "name": "Run BoxPlot",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_plots.py",
      "console": "integratedTerminal",
      "args": [
        "--FECHA_ACTUAL",
        "2025/05/24/14-06",
        "--MODELO",
        "mobilenet"
      ]
    },
    {
      "name": "Run PLOTS finales",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_plots_finales.py",
      "console": "integratedTerminal",
      "args": [
        "--IN",
        "finales_PAINTING",
        "--OUT",
        "painting",
        "--MODELO",
        "mobilenet",
        "--CARPETAS",
        "mem",
        "--MODO_PRINT",
        "juntos"
      ]
    },
    {
      "name": "Run BARPLOT Comparativo",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_diagrama_comparativo.py",
      "console": "integratedTerminal",
      "args": [
        "--CARPETAS",
        "results/csvs/finales_PAINTING/-",
        "results/csvs/finales_PAINTING/mem",
        "results/csvs/finales_PAINTING/mem-f",
        "--NOMBRES_VERSIONES",
        "FIJA",
        "FIJA",
        "LIBRE",
        "--NOMBRES_ALGORITMOS",
        "100%",
        "MEM",
        "MEM-F",
        "--SALIDA",
        "img/finales/painting",
        "--MODO",
        "porcentaje"
      ]
    },
    {
      "name": "BOXPLOT 2 versiones de algoritmos",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_comparar_versiones.py",
      "console": "integratedTerminal",
      "args": [
        "--OUT",
        "PAINTING",
        "--CARPETAS",
        "finales_PAINTING/-",
        "finales_PAINTING/memetico",
        "finales_PAINTING/memetico-libre",
        "--NOMBRES_VERSIONES",
        "FIJA",
        "FIJA",
        "LIBRE",
        "--NOMBRES_ALGORITMOS",
        "100%",
        "MEM",
        "MEM",
        "--METRICAS",
        "Accuracy",
        "--TITULO",
        "BOXPLOT de accuracy de la versi√≥n fija con la Libre"
      ]
    }
  ]
}