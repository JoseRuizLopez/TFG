{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Resumen",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_resumen.py",
      "console": "integratedTerminal",
      "args": []
    },
    {
      "name": "Run Generador",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generator.py",
      "console": "integratedTerminal",
      "args": [
        "--task_id",
        "-1",
        "--MODELO",
        "mobilenet"
      ]
    },
    {
      "name": "Run BoxPlot",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_plots.py",
      "console": "integratedTerminal",
      "args": [
        "--FECHA_ACTUAL",
        "2025/05/24/14-06",
        "--MODELO",
        "mobilenet"
      ]
    },
    {
      "name": "Run PLOTS finales",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_plots_finales.py",
      "console": "integratedTerminal",
      "args": [
        "--IN",
        "finales_PAINTING",
        "--OUT",
        "PAINTING",
        "--MODELO",
        "mobilenet",
        "--CARPETAS",
        "rs",
        "ma",
        "ma-f",
        "--MODO_PRINT",
        "juntos"
      ]
    },
    {
      "name": "Run BARPLOT Comparativo",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_diagrama_comparativo.py",
      "console": "integratedTerminal",
      "args": [
        "--CARPETAS",
        "results/csvs/finales_PAINTING/-",
        "results/csvs/finales_PAINTING/rs",
        "results/csvs/finales_PAINTING/ma",
        "results/csvs/finales_PAINTING/ma-f",
        "--NOMBRES_VERSIONES",
        "FIJA",
        "FIJA",
        "FIJA",
        "LIBRE",
        "--NOMBRES_ALGORITMOS",
        "100%",
        "RS",
        "MA",
        "MA",
        "--SALIDA",
        "img/finales/PAINTING",
        "--MODO",
        "porcentaje"
      ]
    },
    {
      "name": "BOXPLOT 2 versiones de algoritmos",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_comparar_versiones.py",
      "console": "integratedTerminal",
      "args": [
        "--OUT",
        "CIFAR10",
        "--CARPETAS",
        "finales_CIFAR10/ga-am",
        "finales_CIFAR10/ga-am-f",
        "finales_CIFAR10/ma",
        "finales_CIFAR10/ma-f",
        "--NOMBRES_VERSIONES",
        "FIJA",
        "LIBRE",
        "FIJA",
        "LIBRE",
        "--NOMBRES_ALGORITMOS",
        "GA-AM",
        "GA-AM",
        "MA",
        "MA",
        "--METRICAS",
        "Accuracy",
        "--TITULO",
        "BOXPLOT de accuracy de la versi√≥n fija con la libre",
      ]
    }
  ]
}