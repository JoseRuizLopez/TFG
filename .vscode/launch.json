{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Resumen",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_resumen.py",
      "console": "integratedTerminal",
      "args": []
    },
    {
      "name": "Run Generador",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generator.py",
      "console": "integratedTerminal",
      "args": [
        "--task_id",
        "-1",
        "--MODELO",
        "mobilenet"
      ]
    },
    {
      "name": "Run BoxPlot",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_plots.py",
      "console": "integratedTerminal",
      "args": [
        "--FECHA_ACTUAL",
        "2025/05/18/13-14",
        "--MODELO",
        "mobilenet"
      ]
    },
    {
      "name": "Run Final BoxPlot",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_plots_finales.py",
      "console": "integratedTerminal",
      "args": [
        "--IN",
        "finales",
        "--OUT",
        "comparacion_bl-gen_v1",
        "--MODELO",
        "mobilenet",
        "--CARPETAS",
        "-",
        "bl",
        "gen_v1",
        "--MODO",
        "individual",
        "--MODO_PRINT",
        "juntos"
      ]
    }
  ]
}