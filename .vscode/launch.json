{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Resumen",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_resumen.py",
      "console": "integratedTerminal",
      "args": []
    },
    {
      "name": "Run Generador",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generator.py",
      "console": "integratedTerminal",
      "args": [
        "--task_id",
        "-1",
        "--MODELO",
        "mobilenet"
      ]
    },
    {
      "name": "Run BoxPlot",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_plots.py",
      "console": "integratedTerminal",
      "args": [
        "--FECHA_ACTUAL",
        "2025/05/24/14-06",
        "--MODELO",
        "mobilenet"
      ]
    },
    {
      "name": "Run Final BoxPlot",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_plots_finales.py",
      "console": "integratedTerminal",
      "args": [
        "--IN",
        "finales_2",
        "--OUT",
        "comparacion_memetico",
        "--MODELO",
        "mobilenet",
        "--CARPETAS",
        "memetico-libre",
        "--MODO",
        "individual",
        "--MODO_PRINT",
        "juntos"
      ]
    }
  ]
}