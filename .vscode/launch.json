{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Resumen",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_resumen.py",
      "console": "integratedTerminal",
      "args": []
    },
    {
      "name": "Run Generador",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generator.py",
      "console": "integratedTerminal",
      "args": [
        "--task_id",
        "-1",
        "--MODELO",
        "mobilenet"
      ]
    },
    {
      "name": "Run BoxPlot",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_plots.py",
      "console": "integratedTerminal",
      "args": [
        "--FECHA_ACTUAL",
        "2025/05/24/14-06",
        "--MODELO",
        "mobilenet"
      ]
    },
    {
      "name": "Run Final BoxPlot",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_plots_finales.py",
      "console": "integratedTerminal",
      "args": [
        "--IN",
        "finales",
        "--OUT",
        "nueva_formula_mutation",
        "--MODELO",
        "mobilenet",
        "--CARPETAS",
        "gen_v2-2",
        "--MODO",
        "comparar",
        "--MODO_PRINT",
        "juntos"
      ]
    },
    {
      "name": "Run Diagrama Comparativo",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_diagrama_comparativo.py",
      "console": "integratedTerminal",
      "args": [
        "--CARPETAS",
        "results/csvs/comparaciones/nueva_formula_mutation/WC",
        "results/csvs/comparaciones/nueva_formula_mutation/AM",
        "--NOMBRES",
        "WC",
        "AM",
        "--SALIDA",
        "img/finales/nueva_formula_mutation"
      ]
    },
    {
      "name": "Comparar versiones de algoritmos",
      "type": "debugpy",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "program": "${workspaceFolder}/src/generar_comparar_versiones.py",
      "console": "integratedTerminal",
      "args": [
        "--OUT",
        "nueva_formula_mutation",
        "--CARPETAS",
        "comparaciones/nueva_formula_mutation/WC",
        "comparaciones/nueva_formula_mutation/AM",
        "--NOMBRES",
        "WC",
        "AM",
        "--METRICAS",
        "Accuracy",
        "--TITULO",
        "Comparación de accuracy al mejorar el operador de mutación"
      ],
    }
  ]
}